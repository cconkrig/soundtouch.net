{
  "version": "2.3.0-beta",
  "nuGetPackageVersion": {
    "semVer": 2.0
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$",
    "^refs/heads/develop$",
    "^refs/heads/release/\\d+\\.\\d+\\.\\d+$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  },
  "release": {
    "branchName": "release/{version}",
    "firstUnstableTag": "beta"
  }
}